#include<iostream>
using namespace std;

int main() {

	for (int i = 70; i <= 80; i++)
		cout << i << " ";

	cout << endl;

	for (int o = 100; o <= 300; o += 20)
		cout << o << " ";

	cout << endl;

	for (int p = 25; p >= -25; p -= 5)
		cout << p << " ";

	cout << endl;

	for (int s = 10000; s > 10; s /= 5)
		cout << s << " ";

	cout << endl;

}
